	Name = "Parts",
	Default = "input color",
	TextDisappear = true,
	Callback = function(Value)
		for _, part in ipairs(workspace:GetDescendants()) do
        if part:IsA("BasePart") then
            local r = Value
            local g = Value
            local b = Value
            part.BrickColor = BrickColor.new(Color3.new(r, g, b))
        end
    end
	end	  
})

